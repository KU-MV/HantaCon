  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build:osx": "electron-builder --mac",
    "build:linux": "electron-builder --linux --x64 -p never"
  },
  "build": {
    "productName": "Hantacon",
    "linux": {
      "target": ["deb"],
      "category": "Utility",
      "description": "desc test",
      "icon": "img/256x256.png"
    },
    "directories": {
      "buildResources": "img"
    },
    "extraFiles": [
      {
        "from": "./main.nf",
        "to": "."
      },
      {
        "from": "./nextflow.config",
        "to": "."
      },
      {
        "from": "./consensus.nf",
        "to": "."
      },
      {
        "from": "./ref",
        "to": "./ref"
      }
    ],
    "files": [
      "./main.js",
      "./main.html",
      "./preload.js",
      "./jquery-3.7.0.min.js",
      "./worker.js",
      "./worker2.js",
      "./main.css",
      "./img/*"
    ]
  },
  "author": "Grey Ha <greytrace64@gmail.com>",
